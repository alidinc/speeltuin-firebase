{
    "hosting": {
        "public": "hosting",
        "ignore": [
            "firebase.json",
            "**/.*"
        ],
        "headers": [
            {
                "source": "/.well-known/apple-app-site-association",
                "headers": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ]
            }
        ],
        "appAssociation": "NONE",
        "rewrites": []
    }
}
